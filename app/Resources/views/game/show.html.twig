            <tr>
                <th>Forecastwindchill</th>
                <td>{{ game.forecastwindchill }}</td>
            </tr>
            <tr>
                <th>Forecastwindspeed</th>
                <td>{{ game.forecastwindspeed }}</td>
            </tr>
            <tr>
                <th>Forecastwinddirection</th>
                <td>{{ game.forecastwinddirection }}</td>
            </tr>
            <tr>
                <th>Rescheduledfromgameid</th>
                <td>{{ game.rescheduledfromgameid }}</td>
            </tr>
            <tr>
                <th>Runneronfirst</th>
                <td>{{ game.runneronfirst }}</td>
            </tr>
            <tr>
